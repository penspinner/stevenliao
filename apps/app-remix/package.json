{
  "name": "app-remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production remix build",
    "check:eslint": "eslint --color './**/*.{tsx,ts,jsx,js}'",
    "check:format": "prettier './**/*.{tsx,ts,jsx,js}' -c",
    "check:stylelint": "stylelint './**/*.css'",
    "check:typescript": "tsc --noEmit",
    "dev": "cross-env NODE_ENV=development dotenv -e .env -- remix dev",
    "test:unit:cov": "dotenv -e .env -- vitest --coverage",
    "test:unit:watch": "dotenv -e .env -- vitest --watch",
    "test:unit:watch:cov": "pnpm test:unit:watch --coverage"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.16",
    "@react-aria/ssr": "^3.4.1",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-run/serve": "^1.13.0",
    "@remix-run/vercel": "^1.13.0",
    "clsx": "1.2.1",
    "dotenv-cli": "7.0.0",
    "isbot": "^3.6.6",
    "nprogress": "^0.2.0",
    "personal-site": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-flat-routes": "^0.5.7"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.13.0",
    "@testing-library/react": "14.0.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/set-cookie-parser": "^2.4.2",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.5",
    "autoprefixer": "10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "happy-dom": "^8.5.0",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.3",
    "stylelint": "^15.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-prettier": "^2.0.0",
    "tsconfig": "workspace:*",
    "types": "workspace:*",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "vitest": "0.28.5"
  },
  "engines": {
    "node": ">=14"
  }
}
